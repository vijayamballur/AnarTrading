	<parameter name="receiptId" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
     tbl_receipt.`receiptId` AS tbl_receipt_receiptId,
     DATE_FORMAT(tbl_receipt.`voucherDate`,'%d %b %y') AS tbl_receipt_date,
     tbl_receipt.`receivedFrom` AS tbl_receipt_receivedFrom,
     tbl_receipt.`ByCashOrCheckNo` AS tbl_receipt_ByCashOrCheckNo,
     CASE tbl_receipt.`dated`  WHEN '1111-11-11' THEN ' ' ELSE DATE_FORMAT(tbl_receipt.`dated`,'%d %b %y')END AS tbl_receipt_dated,
     tbl_receipt.`bank` AS tbl_receipt_bank,
     tbl_receipt.`being` AS tbl_receipt_being,
     tbl_receipt.`amount` AS tbl_receipt_amount,
     tbl_receipt.`theSumOf` AS tbl_receipt_theSumOf
FROM
     anar.`tbl_receipt` tbl_receipt where tbl_receipt.`receiptId`=$P{receiptId}]]>
	</queryString>
